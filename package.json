{
  "name": "imrememberlee-react-component",
  "version": "0.0.0",
  "description": "Tutorial on how to write React component in ES6 and publishing",
  "main": "src",
  "style": "stylesheets",
  "scripts": {
    "start": "webpack-dev-server",
    "dev": "rimraf lib && babel src --watch --copy-files --source-maps --extensions .es6,.es,.jsx --out-dir lib",
    "compile": "rimraf lib && babel src --copy-files --source-maps --extensions .es6,.es,.jsx --out-dir lib"
  },
  "keywords": [
    "react",
    "react-component"
  ],
  "author": "wyvernnot",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.4.5",
    "babel-loader": "^6.2.1",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "css-loader": "^0.23.1",
    "react": "^15.2.1",
    "react-dom": "^15.2.1",
    "rimraf": "^2.5.0",
    "style-loader": "^0.13.0"
  },
  "directories": {
    "example": "example"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imrememberlee/react-component.git"
  },
  "bugs": {
    "url": "https://github.com/imrememberlee/react-component/issues"
  },
  "homepage": "https://github.com/imrememberlee/react-component#readme"
}
